{
  "name": "wot-blitz-statistics-pro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-i18n:ru": "ng build --output-path=dist/ru --aot --prod --base-href /ru/ --i18n-file=src/i18n/messages.ru.xlf --i18n-format=xlf --i18n-locale=ru",
    "build-i18n:de": "ng build --output-path=dist/de --aot --prod --base-href /de/ --i18n-file=src/i18n/messages.de.xlf --i18n-format=xlf --i18n-locale=de",
    "build-i18n:en": "ng build --output-path=dist --aot --prod --i18n-file=src/i18n/messages.en.xlf --i18n-format=xlf --i18n-locale=en",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:de && npm run build-i18n:ru"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
